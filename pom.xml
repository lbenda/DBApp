<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>cz.lbenda.dataman</groupId>
  <artifactId>dataman-parent</artifactId>
  <version>2.0-SNAPSHOT</version>
  <packaging>pom</packaging>

  <name>dataman-parent</name>

  <url>https://github.com/lbenda/Dataman/wiki</url>
  <scm>
    <connection>scm:git:https://github.com/lbenda/Dataman.git</connection>
    <developerConnection>scm:git:https://github.com/lbenda/Dataman.git</developerConnection>
    <url>https://github.com/lbenda/Dataman.git</url>
  </scm>

  <developers>
    <developer>
      <id>benzin</id>
      <name>Luk치코 Benda</name>
      <email>lbenda@lbenda.cz</email>
      <organization>Luk치코 Benda</organization>
      <organizationUrl>http://www.lbenda.cz</organizationUrl>
    </developer>
  </developers>

  <repositories>
    <repository>
      <id>sonatype</id>
      <name>sonatypenetbeans</name>
      <url>https://oss.sonatype.org/content/repositories/snapshots/</url>
    </repository>
  </repositories>

  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-release-plugin</artifactId>
          <version>2.5.2</version>
        </plugin>
        <plugin>
          <groupId>org.codehaus.mojo</groupId>
          <artifactId>nbm-maven-plugin</artifactId>
          <version>3.13</version>
          <extensions>true</extensions>
          <configuration>
            <brandingToken>${brandingToken}</brandingToken>
            <cluster>${brandingToken}</cluster>
          </configuration>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-compiler-plugin</artifactId>
          <version>3.1</version>
          <configuration>
            <source>1.8</source>
            <target>1.8</target>
          </configuration>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-jar-plugin</artifactId>
          <version>2.4</version>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>

  <modules>
    <module>xsd</module>
    <module>rcp</module>
    <module>dataman</module>
  </modules>

  <properties>
    <netbeans.version>RELEASE80</netbeans.version>
    <brandingToken>dataman</brandingToken>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>

